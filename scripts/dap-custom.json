{
  "definitions": {
    "UpdateCustomBreakpointsRequest": {
      "allOf": [
        {
          "$ref": "#/definitions/Request"
        },
        {
          "type": "object",
          "description": "Updates custom breakpoints (instrumentation, event listeners, etc).",
          "properties": {
            "command": {
              "type": "string",
              "enum": [
                "updateCustomBreakpoints"
              ]
            },
            "arguments": {
              "$ref": "#/definitions/UpdateCustomBreakpointsRequestArguments"
            }
          },
          "required": [
            "command",
            "arguments"
          ]
        }
      ]
    },
    "UpdateCustomBreakpointsRequestArguments": {
      "type": "object",
      "properties": {
        "breakpoints": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/CustomBreakpoint"
          }
        }
      },
      "required": [
        "breakpoints"
      ]
    },
    "UpdateCustomBreakpointsResponse": {
      "allOf": [
        {
          "$ref": "#/definitions/Response"
        },
        {
          "type": "object"
        }
      ]
    },
    "CustomBreakpoint": {
      "type": "object",
      "description": "Custom breakpoint definition.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Breakpoint id."
        },
        "enabled": {
          "type": "boolean",
          "description": "Whether breakpoint should be enabled or disabled."
        },
        "data": {
          "type": "any",
          "description": "Any custom data."
        }
      },
      "required": [
        "id",
        "enabled"
      ]
    },
    "ToggleSourceBlackboxedRequest": {
      "allOf": [
        {
          "$ref": "#/definitions/Request"
        },
        {
          "type": "object",
          "description": "Toggles the blackboxed state of a particular source.",
          "properties": {
            "command": {
              "type": "string",
              "enum": [
                "toggleSourceBlackboxed"
              ]
            },
            "arguments": {
              "$ref": "#/definitions/ToggleSourceBlackboxedRequestArguments"
            }
          },
          "required": [
            "command",
            "arguments"
          ]
        }
      ]
    },
    "ToggleSourceBlackboxedRequestArguments": {
      "type": "object",
      "properties": {
        "source": {
          "description": "The source to be blackboxed or unblackboxed.",
          "$ref": "#/definitions/Source"
        }
      },
      "required": [
        "source"
      ]
    },
    "ToggleSourceBlackboxedResponse": {
      "allOf": [
        {
          "$ref": "#/definitions/Response"
        },
        {
          "type": "object"
        }
      ]
    },
    "ExecutionContextsChangedEvent": {
      "allOf": [
        {
          "$ref": "#/definitions/Event"
        },
        {
          "type": "object",
          "description": "The event indicates that execution contexts have changed.",
          "properties": {
            "event": {
              "type": "string",
              "enum": [
                "executionContextsChanged"
              ]
            },
            "body": {
              "type": "object",
              "properties": {
                "contexts": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/ExecutionContext"
                  }
                }
              },
              "required": [
                "contexts"
              ]
            }
          },
          "required": [
            "event",
            "body"
          ]
        }
      ]
    },
    "ExecutionContext": {
      "type": "object",
      "description": "Execution context definition.",
      "properties": {
        "contextId": {
          "type": "number",
          "description": "Context id."
        },
        "name": {
          "type": "string",
          "description": "Human-readable context name."
        },
        "threadId": {
          "type": "number",
          "description": "Thread id."
        },
        "children": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ExecutionContext"
          }
        }
      },
      "required": [
        "id",
        "threadId",
        "name",
        "children"
      ]
    }
  }
}