{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-msedge",
            "request": "launch",
            "name": "Launch WebView",
            // This executable is built from WebView2Sample.sln
            "runtimeExecutable": "${workspaceFolder}/x64/Debug/WebView2Sample.exe",
            // Navigate to index.html after attaching
            "file": "${workspaceFolder}/index.html",
            // The webview will launch to about:blank
            "urlFilter": "about:blank",
            "useWebView": true
        }
    ]
}