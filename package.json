{
  "name": "js-debug",
  "displayName": "JavaScript Debugger",
  "version": "0.9.0",
  "publisher": "ms-vscode",
  "author": {
    "name": "Microsoft Corporation"
  },
  "keywords": [
    "pwa",
    "javascript",
    "node",
    "chrome",
    "debugger"
  ],
  "preview": true,
  "description": "An extension for debugging Node.js programs and Chrome.",
  "license": "MIT",
  "engines": {
    "vscode": "^1.30.0-insider",
    "node": "^8.9.3"
  },
  "icon": "resources/logo.png",
  "categories": [
    "Debuggers"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-pwa.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-pwa/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:lint"
    }
  },
  "scripts": {
    "compile": "gulp",
    "watch": "gulp watch",
    "fmt": "gulp format",
    "package": "gulp package",
    "publish": "gulp publish",
    "test": "gulp && npm-run-all --parallel test:golden test:lint",
    "test:golden": "node ./out/src/test/runTest.js",
    "test:lint": "gulp lint"
  },
  "dependencies": {
    "@c4312/chromehash": "^0.2.0",
    "color": "^3.1.2",
    "default-browser": "^2.0.1",
    "execa": "^4.0.0",
    "glob-stream": "^6.1.0",
    "inversify": "^5.0.1",
    "js-beautify": "^1.11.0",
    "jsonc-parser": "^2.2.1",
    "micromatch": "^4.0.2",
    "reflect-metadata": "^0.1.13",
    "source-map": "^0.7.3",
    "split2": "^3.1.1",
    "typescript": "^3.8.3",
    "vscode-js-debug-browsers": "^1.0.0",
    "vscode-nls": "^4.1.2",
    "ws": "^7.2.3"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "avoid",
    "printWidth": 100,
    "tabWidth": 2
  },
  "devDependencies": {
    "@mixer/parallel-prettier": "^2.0.0",
    "@pptr/testrunner": "^0.8.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-string": "^1.4.2",
    "@types/chai-subset": "^1.3.3",
    "@types/color": "^3.0.1",
    "@types/debug": "^4.1.5",
    "@types/del": "^4.0.0",
    "@types/express": "^4.17.4",
    "@types/glob": "^7.1.1",
    "@types/glob-stream": "^6.1.0",
    "@types/gulp": "^4.0.6",
    "@types/js-beautify": "^1.8.2",
    "@types/json-schema": "^7.0.4",
    "@types/lodash": "^4.14.149",
    "@types/long": "^4.0.1",
    "@types/micromatch": "^4.0.1",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^1.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/sinon": "^9.0.0",
    "@types/split2": "^2.1.6",
    "@types/stream-buffers": "^3.0.3",
    "@types/tmp": "^0.1.0",
    "@types/ws": "^7.2.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "chai-subset": "^1.6.0",
    "deepmerge": "^4.2.2",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-header": "^3.0.0",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-filter": "^6.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "gulp-util": "^3.0.8",
    "husky": "^4.2.3",
    "lodash": "^4.17.15",
    "merge2": "^1.3.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "mocha": "^7.1.1",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi-reporters": "^1.1.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "playwright": "^0.12.1",
    "prettier": "^2.0.4",
    "sinon": "^9.0.1",
    "stream-buffers": "^3.0.2",
    "ts-node": "^8.8.2",
    "vsce": "^1.75.0",
    "vscode-debugadapter-testsupport": "^1.40.3",
    "vscode-debugprotocol": "^1.40.0",
    "vscode-nls-dev": "^3.3.1",
    "vscode-test": "^1.3.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "main": "./src/extension.js",
  "enableProposedApi": true,
  "activationEvents": [
    "onLanguage:json",
    "onDebugInitialConfigurations",
    "onDebugResolve:node",
    "onDebugResolve:extensionHost",
    "onDebugResolve:chrome"
  ],
  "extensionKind": [
    "workspace"
  ],
  "contributes": {
    "views": {
      "debug": [
        {
          "id": "jsBrowserBreakpoints",
          "name": "Browser breakpoints",
          "when": "debugConfigurationType == 'NAMESPACE(chrome)'"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "debug",
        "contents": "%debug.terminal.welcome%",
        "when": "debugStartLanguage == javascript || debugStartLanguage == typescript"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "extension.NAMESPACE(node-debug).prettyPrint",
          "title": "%pretty.print.script%",
          "when": "inDebugMode && debugType == NAMESPACE(chrome) && editorLangId == javascript"
        },
        {
          "command": "extension.NAMESPACE(node-debug).startProfile",
          "title": "%profile.start%",
          "when": "inDebugMode && debugType == NAMESPACE(chrome)"
        },
        {
          "command": "extension.NAMESPACE(node-debug).stopProfile",
          "title": "%profile.stop%",
          "when": "inDebugMode && debugType == NAMESPACE(chrome) && jsDebugIsProfiling"
        }
      ],
      "debug/callstack/context": [
        {
          "command": "extension.NAMESPACE(node-debug).toggleSkippingFile",
          "group": "navigation",
          "when": "inDebugMode && debugType == NAMESPACE(node) && callStackItemType == 'stackFrame'"
        },
        {
          "command": "extension.NAMESPACE(node-debug).toggleSkippingFile",
          "group": "navigation",
          "when": "inDebugMode && debugType == NAMESPACE(chrome) && callStackItemType == 'session'"
        },
        {
          "command": "extension.NAMESPACE(node-debug).startProfile",
          "group": "navigation",
          "when": "debugType == NAMESPACE(chrome) && !jsDebugIsProfiling"
        },
        {
          "command": "extension.NAMESPACE(node-debug).stopProfile",
          "group": "navigation",
          "when": "debugType == NAMESPACE(chrome) && jsDebugIsProfiling"
        }
      ],
      "debug/toolBar": [
        {
          "command": "extension.NAMESPACE(node-debug).stopProfile",
          "when": "debugType == NAMESPACE(chrome) && jsDebugIsProfiling"
        }
      ],
      "view/title": [
        {
          "command": "extension.NAMESPACE(chrome-debug).addCustomBreakpoints",
          "when": "view == jsBrowserBreakpoints",
          "group": "navigation"
        },
        {
          "command": "extension.NAMESPACE(chrome-debug).removeAllCustomBreakpoints",
          "when": "view == jsBrowserBreakpoints",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "extension.NAMESPACE(chrome-debug).removeCustomBreakpoint",
          "when": "view == jsBrowserBreakpoints",
          "group": "inline"
        },
        {
          "command": "extension.NAMESPACE(chrome-debug).addCustomBreakpoints",
          "when": "view == jsBrowserBreakpoints"
        },
        {
          "command": "extension.NAMESPACE(chrome-debug).removeCustomBreakpoint",
          "when": "view == jsBrowserBreakpoints"
        }
      ]
    },
    "breakpoints": [
      {
        "language": "javascript"
      },
      {
        "language": "typescript"
      },
      {
        "language": "typescriptreact"
      },
      {
        "language": "javascriptreact"
      },
      {
        "language": "fsharp"
      },
      {
        "language": "html"
      }
    ]
  }
}
