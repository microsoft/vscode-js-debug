{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2017"],
		"noImplicitAny": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "outDir": "../out/out",
    "strict": true,
    "noUnusedLocals": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "types": ["node", "chai-subset", "mocha"],
  }
}
