{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2017"],
    "noImplicitAny": false,
    "removeComments": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "inlineSourceMap": false,
    "sourceMap": true,
    "outDir": "../out",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "noUnusedParameters": false
  }
}
